{
	"root": true,
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "prettier"],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"rules": {
		"semi": "error",
		"no-unused-vars": [
			"error",
			{ "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
		]
	}
}
